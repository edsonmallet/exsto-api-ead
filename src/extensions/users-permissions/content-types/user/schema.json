{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "company": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::company.company"
    },
    "notifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::notification.notification",
      "mappedBy": "users_permissions_users"
    },
    "certificate": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::certificate.certificate",
      "inversedBy": "users_permissions_users"
    },
    "badges": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::badge.badge",
      "mappedBy": "users_permissions_users"
    },
    "usernamePublic": {
      "type": "string"
    },
    "birthDate": {
      "type": "date"
    },
    "whatsapp": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "minibio": {
      "type": "richtext"
    },
    "avatar": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "document": {
      "type": "string"
    },
    "addresses": {
      "displayName": "userAddress",
      "type": "component",
      "repeatable": true,
      "component": "modules.user-address"
    }
  }
}
