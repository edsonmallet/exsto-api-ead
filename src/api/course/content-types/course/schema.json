      "target": "api::course-module.course-module",
      "inversedBy": "courses"
    },
    "notifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::notification.notification",
      "mappedBy": "courses"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "courses"
    },
